{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom --watchAll",
    "webpack": "./node_modules/.bin/webpack"
  },
  "dependencies": {
    "@types/autoprefixer": "^6.7.3",
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.2",
    "@types/chalk": "^2.2.0",
    "@types/dotenv": "^4.0.3",
    "@types/extract-text-webpack-plugin": "^3.0.3",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.1",
    "@types/fs-extra": "^5.0.3",
    "@types/graphql": "^0.13.3",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/object-assign": "^4.0.30",
    "@types/promise": "^7.1.30",
    "@types/resolve": "0.0.8",
    "@types/sw-precache-webpack-plugin": "^0.11.0",
    "apollo-boost": "^0.1.10",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.4",
    "autoprefixer": "7.1.6",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "emotion": "^9.2.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "^22.4.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.1",
    "react-emotion": "^9.2.4",
    "resolve": "1.6.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.1.5",
    "@types/node": "^10.5.1",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/webpack": "^4.4.4",
    "awesome-typescript-loader": "^5.2.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "husky": "^0.14.3",
    "jest-emotion": "^9.2.4",
    "jest-environment-node": "^23.2.0",
    "nyc": "^12.0.2",
    "precise-commits": "^1.0.2",
    "prettier": "^1.13.7",
    "redbox-react": "^1.6.0",
    "typescript": "^2.9.2",
    "typescript-snapshots-plugin": "^1.2.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  },
  "nyc": {
    "lines": 80,
    "functions": 80,
    "branches": 80,
    "check-coverage": true,
    "exclude": [
      "node_modules",
      "bin",
      "coverage",
      "__tests__/*",
      "**/*.test.*"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precise-commits && npm run quality"
    }
  }
}
